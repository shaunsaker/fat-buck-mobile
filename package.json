{
  "name": "FatBuck",
  "version": "1.0.0",
  "build": "1",
  "code": "1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "yarn android --variant release",
    "ios": "react-native run-ios",
    "ios:release": "yarn ios --configuration Release",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "npx pod-install",
    "prerelease": "yarn lint && yarn test",
    "release": "yarn ts-node ./scripts/deploy.ts",
    "prerelease:code": "yarn lint && yarn test",
    "release:code": "yarn ts-node ./scripts/deploy.ts --code",
    "deploy:android": "cd android && bundle exec fastlane deploy",
    "deploy:ios": "cd ios && bundle exec fastlane deploy",
    "deploy:code": "npx code-push release-react fat-buck-ios ios -d Production && PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && npx code-push release-react fat-buck-android android -d Production -t $PACKAGE_VERSION"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^8.4.2",
    "@react-native-firebase/auth": "^9.2.2",
    "@react-native-firebase/firestore": "^7.8.1",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@sentry/react-native": "^1.7.2",
    "countries-information": "^0.9.3",
    "libphonenumber-js": "^1.7.58",
    "moment": "^2.28.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-code-push": "^6.3.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.0",
    "react-native-restart": "^0.0.17",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-side-menu": "^1.1.3",
    "react-native-simple-animators": "^2.2.6",
    "react-native-snackbar": "^2.2.3",
    "react-native-svg": "^12.1.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "tinycolor2": "^1.4.1",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-native-side-menu": "^1.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.3",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.1.1",
    "react-dom": "^17.0.0-rc.1",
    "react-is": "^16.13.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3",
    "yargs": "^16.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
